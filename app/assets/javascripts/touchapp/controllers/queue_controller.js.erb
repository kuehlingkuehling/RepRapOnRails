touchApp.controller('QueueController', function($scope, $modal, Printer, CommonCode){
  console.log("Running QueueController");  

  Printer.menuDisabled = false; 

 $scope.$watch(function(){ return Printer.print; }, function(newValue){
    if (( Printer.print.state == 2) || ( Printer.print.state == 3 ) || ( Printer.print.state == 5 )) {
      $scope.printing = true;
    } else {
      $scope.printing = false;      
    }    
  }, true);    
  
  $scope.$watch(function(){ return Printer.print; }, function(newValue){
    $scope.print = Printer.print;
    if (Printer.print.state == 1) {
      $scope.idle = true;
    } else {
      $scope.idle = false;      
    }    
  }, true);    

  $scope.$watch(function(){ return Printer.printjobs; }, function(){
    $scope.printjobs = Printer.printjobs;
  },true);  




  
     
});